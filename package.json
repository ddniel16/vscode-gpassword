{
  "activationEvents": [],
  "author": {
    "name": "ddniel16",
    "url": "https://github.com/ddniel16"
  },
  "bugs": {
    "url": "https://github.com/ddniel16/vscode-gpassword/issues"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "category": "gPassword",
        "command": "gpassword.Letters",
        "title": "Letters only"
      },
      {
        "category": "gPassword",
        "command": "gpassword.LettersNumbers",
        "title": "Letters and numbers"
      },
      {
        "category": "gPassword",
        "command": "gpassword.AllCharacters",
        "title": "Letters, Numbers & Symbols"
      },
      {
        "category": "gPassword",
        "command": "gpassword.passwordToBasicAuth",
        "title": "Replace password to hash of Basic Auth (bcrypt.hashSync)"
      },
      {
        "category": "gPassword",
        "command": "gpassword.BaseEncode",
        "title": "Base64 Encode"
      },
      {
        "category": "gPassword",
        "command": "gpassword.BaseDecode",
        "title": "Base64 Decode"
      },
      {
        "category": "gPassword",
        "command": "gpassword.WordPressSaltYml",
        "title": "WordPress Salt YML"
      },
      {
        "category": "gPassword",
        "command": "gpassword.WordPressSaltEnv",
        "title": "WordPress Salt ENV"
      },
      {
        "category": "gPassword",
        "command": "gpassword.StrapiYml",
        "title": "Strapi Tokens YML"
      },
      {
        "category": "gPassword",
        "command": "gpassword.StrapiEnv",
        "title": "Strapi Tokens ENV"
      },
      {
        "category": "gPassword",
        "command": "gpassword.JWT",
        "title": "JWT Decode"
      }
    ],
    "configuration": [
      {
        "title": "gPassword: General",
        "order": 1,
        "properties": {
          "gpassword.randomLengthMax": {
            "default": 25,
            "description": "Maximum characters for random password (Command & Submenu)",
            "maximum": 55,
            "minimum": 16,
            "type": "integer"
          },
          "gpassword.randomLengthMin": {
            "default": 20,
            "description": "Minimum characters for random password (Command & Submenu)",
            "maximum": 54,
            "minimum": 15,
            "type": "integer"
          }
        }
      },
      {
        "title": "gPassword: Password Generator (Sidebar)",
        "order": 2,
        "properties": {
          "gpassword.passwordGeneratorLength": {
            "default": 20,
            "description": "Default length for password generator",
            "maximum": 55,
            "minimum": 10,
            "type": "integer"
          },
          "gpassword.passwordGeneratorDefault.includeNumbers": {
            "default": true,
            "description": "Include numbers in password",
            "type": "boolean"
          },
          "gpassword.passwordGeneratorDefault.includeSymbols": {
            "default": true,
            "description": "Include symbols in password",
            "type": "boolean"
          },
          "gpassword.passwordGeneratorDefault.includeUppercase": {
            "default": true,
            "description": "Include uppercase letters in password",
            "type": "boolean"
          }
        }
      }
    ],
    "views": {
      "passwordGenerator": [
        {
          "id": "passwordGenerator.password",
          "name": "Password Generator",
          "type": "webview"
        },
        {
          "id": "passwordGenerator.jwt",
          "name": "JWT Decoder & Validator",
          "type": "webview"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "icon": "media/lock.svg",
          "id": "passwordGenerator",
          "title": "gPassword & Encoder Tools"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "group": "gpassword",
          "submenu": "gpassword.submenu",
          "when": "editorTextFocus && !editorReadonly"
        }
      ],
      "gpassword.submenu": [
        {
          "command": "gpassword.BaseDecode",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && editorHasSelection"
        },
        {
          "command": "gpassword.BaseEncode",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && editorHasSelection"
        },
        {
          "command": "gpassword.passwordToBasicAuth",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && editorHasSelection"
        },
        {
          "command": "gpassword.JWT",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && editorHasSelection"
        },
        {
          "command": "gpassword.AllCharacters",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.LettersNumbers",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.Letters",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.WordPressSaltYml",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.WordPressSaltEnv",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.StrapiYml",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.StrapiEnv",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        }
      ]
    },
    "submenus": [
      {
        "id": "gpassword.submenu",
        "label": "gPassword"
      }
    ]
  },
  "dependencies": {
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "bcrypt": "^6.0.0"
  },
  "description": "Generator password & Encoder Tools is a one-stop solution for strong password generation and encoding tools.",
  "devDependencies": {
    "@types/assert": "^1.5.11",
    "@types/bcrypt": "^6.0.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.16.15",
    "@types/sinon": "^21.0.0",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "sinon": "^21.0.1",
    "typescript": "^5.9.3"
  },
  "displayName": "Generator password & Encoder Tools",
  "engines": {
    "vscode": "^1.101.0"
  },
  "galleryBanner": {
    "color": "#0C4A6E",
    "theme": "dark"
  },
  "homepage": "https://github.com/ddniel16/vscode-gpassword#readme",
  "icon": "imgs/icon.png",
  "keywords": [
    "security",
    "password",
    "bcrypt",
    "base64",
    "basic auth",
    "jwt decode",
    "WordPress Salt"
  ],
  "license": "GPL-3.0-or-later",
  "main": "./out/extension.js",
  "name": "gpassword",
  "pricing": "Free",
  "publisher": "ddniel16",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddniel16/vscode-gpassword"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.1.1"
}
