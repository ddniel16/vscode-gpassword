{
  "activationEvents": [],
  "author": {
    "name": "ddniel16",
    "url": "https://github.com/ddniel16"
  },
  "categories": [
    "Other"
  ],
  "contributes": {
    "commands": [
      {
        "category": "gPassword",
        "command": "gpassword.Letters",
        "title": "Letters only"
      },
      {
        "category": "gPassword",
        "command": "gpassword.LettersNumbers",
        "title": "Letters and numbers"
      },
      {
        "category": "gPassword",
        "command": "gpassword.AllCharacters",
        "title": "Letters, Numbers & Symbols"
      },
      {
        "category": "gPassword",
        "command": "gpassword.passwordToBasicAuth",
        "title": "Replace password to hash of Basic Auth (bcrypt.hashSync)"
      },
      {
        "category": "gPassword",
        "command": "gpassword.BaseEncode",
        "title": "Base64 Encode"
      },
      {
        "category": "gPassword",
        "command": "gpassword.BaseDecode",
        "title": "Base64 Decode"
      },
      {
        "category": "gPassword",
        "command": "gpassword.WordPressSaltYml",
        "title": "WordPress Salt YML"
      },
      {
        "category": "gPassword",
        "command": "gpassword.WordPressSaltEnv",
        "title": "WordPress Salt ENV"
      },
      {
        "category": "gPassword",
        "command": "gpassword.StrapiYml",
        "title": "Strapi Tokens YML"
      },
      {
        "category": "gPassword",
        "command": "gpassword.StrapiEnv",
        "title": "Strapi Tokens ENV"
      },
      {
        "category": "gPassword",
        "command": "gpassword.JWT",
        "title": "JWT Decode"
      }
    ],
    "configuration": {
      "properties": {
        "gpassword.randomLengthMax": {
          "default": 25,
          "description": "Maximum characters",
          "maximum": 55,
          "minimum": 16,
          "type": "integer"
        },
        "gpassword.randomLengthMin": {
          "default": 20,
          "description": "Minimum characters",
          "maximum": 54,
          "minimum": 15,
          "type": "integer"
        }
      },
      "title": "gPassword"
    },
    "menus": {
      "editor/context": [
        {
          "group": "gpassword",
          "submenu": "gpassword.submenu",
          "when": "editorTextFocus && !editorReadonly"
        }
      ],
      "gpassword.submenu": [
        {
          "command": "gpassword.BaseDecode",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && editorHasSelection"
        },
        {
          "command": "gpassword.BaseEncode",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && editorHasSelection"
        },
        {
          "command": "gpassword.passwordToBasicAuth",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && editorHasSelection"
        },
        {
          "command": "gpassword.AllCharacters",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.LettersNumbers",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        },
        {
          "command": "gpassword.Letters",
          "group": "gpassword",
          "when": "editorTextFocus && !editorReadonly && !editorHasSelection"
        }
      ]
    },
    "submenus": [
      {
        "id": "gpassword.submenu",
        "label": "gPassword"
      }
    ]
  },
  "dependencies": {
    "bcrypt": "^6.0.0"
  },
  "description": "Generator password & Encoder Tools is a one-stop solution for strong password generation and encoding tools.",
  "devDependencies": {
    "@types/assert": "^1.5.11",
    "@types/bcrypt": "^5.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.0.4",
    "@types/sinon": "^17.0.4",
    "@types/vscode": "^1.101.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.29.0",
    "sinon": "^21.0.0",
    "typescript": "^5.8.3"
  },
  "displayName": "Generator password & Encoder Tools",
  "engines": {
    "vscode": "^1.101.0"
  },
  "icon": "imgs/icon.png",
  "keywords": [
    "security",
    "password",
    "bcrypt",
    "base64",
    "basic auth",
    "jwt decode",
    "WordPress Salt"
  ],
  "license": "GPL-3.0-or-later",
  "main": "./out/extension.js",
  "name": "gpassword",
  "pricing": "Free",
  "publisher": "ddniel16",
  "repository": {
    "type": "git",
    "url": "https://github.com/ddniel16/vscode-gpassword"
  },
  "scripts": {
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "pretest": "npm run compile && npm run lint",
    "test": "vscode-test",
    "vscode:prepublish": "npm run compile",
    "watch": "tsc -watch -p ./"
  },
  "version": "0.0.7"
}
